<pretty-config 	xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
				xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation="http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">
    
    <url-mapping id="inicio">
        <pattern value="/inicio/" />
        <view-id value="/content/inicio.xhtml" />
    </url-mapping>
    <url-mapping id="login">
        <pattern value="/" />
        <view-id value="/index.xhtml" />
    </url-mapping>
    <url-mapping id="timeout">
        <pattern value="/timeout" />
        <view-id value="/content/timeout.xhtml" />
    </url-mapping>
</pretty-config>
